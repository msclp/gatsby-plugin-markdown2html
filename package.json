{
  "name": "gatsby-plugin-markdown2html",
  "version": "1.0.1",
  "description": "Gatsby plugin for converting markdown to HTML",
  "repository": {
    "type": "git",
    "url": "https://github.com/msclp/gatsby-plugin-markdown2html"
  },
  "homepage": "https://github.com/msclp/gatsby-plugin-markdown2html",
  "bugs": "https://github.com/msclp/gatsby-plugin-markdown2html/issues",
  "author": "Lukasz Palasz <lukasz.palasz@atombank.co.uk>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src --out-dir . --extensions \".ts\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "keywords": ["gatsby", "plugin", "markdown", "html"],
  "dependencies": {
    "gatsby": "^5.8.1",
    "showdown": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.13.15",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.0",
    "@types/showdown": "^2.0.0",
    "babel-jest": "^29.5.0",
    "babel-preset-gatsby": "^3.8.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
